# HyperLiquid Configuration
# ============================================================================
# IMPORTANT: This is an example file. Copy to .env and fill in your values
# Never commit .env file to git!
# ============================================================================

# HyperLiquid Private Key (Ethereum wallet)
# Format: 64 hex characters (with or without 0x prefix)
# Example: HYPERLIQUID_PRIVATE_KEY=0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef
HYPERLIQUID_PRIVATE_KEY=your_private_key_here

# LLM API Keys (optional for testing, required for AI trading)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
QWEN_API_KEY=your_qwen_api_key_here

# Database Configuration (optional for testing)
DB_USER=trading_bot
DB_PASSWORD=your_secure_password
DB_HOST=localhost
DB_PORT=5432
DB_NAME=trading_bot

# Telegram Alerts (optional)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# Email Alerts (optional)
EMAIL_USERNAME=your_email@gmail.com
EMAIL_PASSWORD=your_email_app_password
EMAIL_FROM=your_email@gmail.com
EMAIL_TO=your_email@gmail.com
